  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost1",
      "url": "http://localhost:8000",
      "webRoot": "${workspaceFolder}",
      "breakOnLoad": false,
      "sourceMapPathOverrides": {
        "../*": "${webRoot}/*"
      }    
    }
  ]
}